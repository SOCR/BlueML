      // The Progress Bar
      link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css')
      .container
        h2 You are in Progress: 
        .progress
          .progress-bar.progress-bar-striped.table-active(role='progressbar', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100', style='width:25%')
            span.sr-only
      br
      // Web App Content
      .app-container
        .app-nav-bar
          .row
            .col-xl-12.text-center
              ul.nav.nav-pills.nav-justified
                li#training_data_page.table-active.nav-item(role='presentation')
                  a.nav-link(href='#') Select Training Data
                li#load_data_page.app-nav.nav-item(role='presentation')
                  a.nav-link(href='#') Upload Test Data
                li#analysis_page.app-nav.nav-item(role='presentation')
                  a.nav-link(href='#') Analysis
                li#results_page.app-nav.nav-item(role='presentation')
                  a.nav-link(href='#') Results
        br
        .app-interface-container
          .row
            .col-xl-12.text-center
              .module(id='Training Data Page')
                // Training Data Page goes here
                h1 Select Training Data
              .module(id='Load Data Page')
                // Load Data Page goes here
                h1 Upload Your Data
                .file_upload
                  .table-responsive
                    table#file_upload_table.table.table-bordered
                      thead
                        tr
                          th File Number
                          th File
                          th File Name
                          th Delete
                      tbody
                        tr
                          td 1
                          td
                            input#file_upload_button(type='file', name='file')
                          td
                          td
                            button#delete_file_upload.btn.btn-secondary.btn-sm(type='button', onclick='deletetr(this)')
                              span.glyphicon.glyphicon-minus
                              |  Remove
                    a#add_another_upload_button.btn.btn-secondary.btn-lg(disabled='disabled')
                      | Add Another File
                    p
                      button#upload_button.btn.btn-secondary.btn-lg(type='button', name='Upload_ECG') Upload ECG File
              .module(id='Analysis Page')
                // Analysis Page goes here
                h1 Select Algorithm Complexity
                b   Less Accurate/Faster  
                input#slide
                b   More Accurate/Slower  
                br
                br
                .dropdown(style='width: 125px;margin:auto')
                  button.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
                    | Advanced Options	
                    span.caret
                  ul.dropdown-menu
                    li.dropdown-item
                      a(href='#') Option 1
                    li.dropdown-item
                      a(href='#') Option 2
                    li.dropdown-item
                      a(href='#') Option 3
                h1 Select Features for Classification
                .containerCheck(ng-controller='FeatureCtrl')
                  label.form-check(ng-repeat='(feature,enabled) in features')
                    input(type='checkbox', ng-model='features[feature]')
                    |  {{feature}}
              .module(id='Result Page')
                // Result Page goes here
                h1 Result
      // /.row
    button#previous_module.btn.btn-outline-primary(type='button') Previous
